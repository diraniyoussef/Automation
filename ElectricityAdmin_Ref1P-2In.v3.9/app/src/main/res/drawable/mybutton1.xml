<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#dde5e4" />
            <gradient android:angle="-90" android:startColor="#dde5e4" android:endColor="#90a9a4"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#f4f6f6" />
            <solid android:color="#b1c3bf"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#c7d4d1" />
            <gradient android:angle="-90" android:startColor="#dde5e4" android:endColor="#b1c3bf" />
        </shape>
    </item>
</selector>